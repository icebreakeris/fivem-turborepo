{
  "name": "@fivem-turborepo/ts-react-template",
  "version": "1.0.0",
  "license": "MIT",
  "description": "happy coding!",
  "type": "module",
  "scripts": {
    "build": "concurrently \"yarn build:script\" \"yarn build:ui\"",
    "build:script": "node build.js",
    "build:ui": "cd src/client/ui && yarn build",
    "dev": "concurrently \"yarn dev:script\" \"yarn dev:ui\"",
    "dev:script": "set IS_WATCH=true && node build.js",
    "dev:ui": "cd src/client/ui && yarn build --watch",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@citizenfx/client": "^2.0.6269-1",
    "@citizenfx/server": "^2.0.6269-1",
    "@types/node": "^18.15.1",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "esbuild": "^0.17.11",
    "eslint": "^8.36.0",
    "typescript": "^4.9.3",
    "concurrently": "^7.6.0"
  }
}
